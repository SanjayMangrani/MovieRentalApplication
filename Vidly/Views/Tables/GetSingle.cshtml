@model Microsoft.WindowsAzure.Storage.Table.TableResult
@{
    ViewBag.Title = "GetSingle";
}

<h2>Get Single results</h2>

<table border="1">
    <tr>
        <th>HTTP result</th>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
    </tr>
    <tr>
        <td>@Model.HttpStatusCode</td>
        <td>@((Model.Result as Vidly.Models.CustomerEntity).RowKey)</td>
        <td>@((Model.Result as Vidly.Models.CustomerEntity).PartitionKey)</td>
        <td>@((Model.Result as Vidly.Models.CustomerEntity).Email)</td>
    </tr>
</table>